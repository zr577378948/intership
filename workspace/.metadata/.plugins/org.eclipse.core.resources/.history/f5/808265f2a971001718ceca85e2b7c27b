package com.ouya.zr;

public class gouzao {
	
	public static void main(String[] args) {
		Person p = new Person(String name);
		p.info();
	}
}
class Person{
	String name ;
	int age ;
	
	public Person(String a){
		name = a ;
	}
	
	public void info(){
		System.out.println("name:"+ name +",age:"+age);
	}
}