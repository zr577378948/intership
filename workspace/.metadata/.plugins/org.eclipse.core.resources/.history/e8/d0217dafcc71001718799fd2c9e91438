package com.ouya.zr;

public class StaticTest {
	public static void main(String[] args) {
		
		St s = new St();
		s.age = 4;
		s.info();
		St.age = 5;
	}

}

class St{
	static int age;
	String name;
	
	public void  info(){
		
		System.out.println("name:"+name+",age:" + age);
	}
}