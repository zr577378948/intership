package com.ouya.zr;
/**
 * 定义：某一种实物存在多种形态
 * @author Administrator
 *
 */
public class Demo {

}

abstract class Animal{
	abstract void eat();
}

class Dog extends Animal{

	@Override
	void eat() {
		System.out.println("吃好吃的");
	}
	void look(){
		System.out.println("看家");
	}
	
	
}

class Cat extends Animal{

	@Override
	void eat() {
		System.out.println("吃老鼠");
	}
	void look(){
		System.out.println("看家");
	}
	
	
}