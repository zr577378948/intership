package com.ouya.zr;

public class shuzu {

	public static void main(String[] args) {

		String names[] = new String[]{"zhangrui","fengcehngcheng","yanbin"};
		int[] aa = new int[3];
		
		aa[0] = 1;
		aa[1] = 2;
		aa[2] = 3;
		
		for(int i = 0;i<aa.length;i++){
			System.out.println(aa[i]);
		}
		
		for(int i = 0;i<names.length;i++){
			System.out.println(aa[i]);
		}
		
	}

}
