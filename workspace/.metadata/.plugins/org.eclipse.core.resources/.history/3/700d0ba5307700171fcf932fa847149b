package com.ouya.zr.io;

import java.io.File;

/**
 * 删除文件
 * delete();
 * 删除抽象类指定路径的文件
 * @author Administrator
 *exists();判断文件是否存在
 */

public class TestIO1 {

	public static void main(String[] args) {
		File f = new File("e:"+File.separator+"test.txt");
		f.exists();
		f.delete();
	}
}
