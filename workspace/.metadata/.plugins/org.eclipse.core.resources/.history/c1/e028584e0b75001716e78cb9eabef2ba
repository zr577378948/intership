class GeometricObject{
	protected String color;
	protected double weigt;
	public GeometricObject(String color, double weigt) {
		this.color = color;
		this.weigt = weigt;
	}
	
	
}

class Circle extends GeometricObject{
	private double radius;
	
	public Circle(String color, double weigt,double radius){
		super(color,weight);
		this.radius = radius;
	}
}