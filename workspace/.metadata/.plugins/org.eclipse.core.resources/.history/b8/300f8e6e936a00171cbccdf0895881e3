package com.ouya.zr;
/**
 * 数据类型转换:从小到大char,byte,short=>int=>long=>float=>double
 * char,byte,short运算时,自动转化为int类型
 * @author Administrator
 *
 */
public class zhuanhuan {

	public static void main(String[] args) {
		int a=5;
		long d=3l;
		long f=a+d;
		
		double g=1.5;
		double r=d+g;
		System.out.println(f);
		System.out.println(r);

	}

}
