package com.ouya.zr;
//接口跟具体的类指尖存在多态性
public class TestInterface {

}

interface Runner{
	public abstract void run();
	
}
interface Swimmer{
	void swim();
}
interface Filer{
	void fly();
}

class Duck implements Runner,Swimmer,Filer{

	@Override
	public void fly() {

		System.out.println("丑小鸭也可以变成白天鹅");
	}

	@Override
	public void swim() {

		System.out.println("红掌波清波");
	}

	@Override
	public void run() {

		System.out.println("鸭子屁股妞妞");
	}
	
}