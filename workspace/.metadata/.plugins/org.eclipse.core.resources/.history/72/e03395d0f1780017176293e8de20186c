package com.ouya.zr.collection;

import java.util.HashSet;
import java.util.Set;

/*
 * set接口：存储顺序无序，不重复
 */
public class TestSet {

	public static void main(String[] args) {
		String str = "你好";
		String str1 = "你好吗？";
		String str2 = "很好，谢谢";
		System.out.println(str==str1);//比值
		
		String Str = new String("nihao");
		String Str1 = new String("nihaoma?");
		String Str2 = new String("henhao,xiexie");
		System.out.println(Str==Str1);//比地址
		
		Set set = new HashSet<>();
		set.add(str);
		set.add(str1);
		set.add(str2);
		
		set.add(Str);
		set.add(Str1);
		set.add(Str2);
		System.out.println(set);
		
	}
}
